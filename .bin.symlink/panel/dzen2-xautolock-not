#!/bin/sh

# Import settings
realpath=$(cd `dirname "${BASH_SOURCE[0]}"` && pwd)
. $realpath/panel-settings

 (
echo "â˜¼"
 ) | dzen2 \
    -p \
    -x "1550" \
    -y "850" \
    -w "45" \
    -h "45" \
    -l "0" \
    -sa "c" \
